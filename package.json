{
  "name": "poc-lifecycle",
  "version": "1.0.0",
  "description": "Node.js application",
  "keywords": [
    "actions",
    "nodejs",
    "express",
    "husky",
    "template",
    "backend"
  ],
  "private": true,
  "author": "Andres Mauricio Acosta Pulido, <amacostapulido@gmail.com>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc",
    "build:stage": "tsc",
    "build:prod": "tsc",
    "start": "npm run build && node ./build/bin/www.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test": "npm run lint && npm run test:only",
    "coverage": "export $(cat test/.env-test | xargs) && nyc _mocha --timeout 10000 --require ts-node/register 'test/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf .nyc_output coverage build .scannerwork performance/*.clinic-doctor*"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "express-actuator": "^1.8.4",
    "mongoose": "^7.1.1",
    "morgan": "^1.10.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.3.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/express-actuator": "^1.8.0",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.0.0",
    "@types/sinon": "^10.0.14",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "debug": "^4.3.4",
    "mocha": "^10.2.0",
    "nock": "^13.3.1",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "sinon": "^15.0.4",
    "sonarqube-scanner": "^3.0.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  }
}